{
    "name": "bc/installer",
    "description": "Laravel web installer",
    "license": "MIT",
    "authors": [
        {
            "name": "BookingCore",
            "email": "contact@bookingcore.co"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "BC\\Installer\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "BC\\Installer\\Providers\\LaravelInstallerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}